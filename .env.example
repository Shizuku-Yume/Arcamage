# Arcamage Environment Variables
# Copy this file to .env and configure as needed

# Application settings
ARCAMAGE_DEBUG=false
ARCAMAGE_LOG_LEVEL=INFO

# File upload limits
ARCAMAGE_MAX_UPLOAD_MB=20

# HTTP client timeout (seconds)
ARCAMAGE_HTTP_TIMEOUT=30

# Log redaction (hide sensitive data in logs)
ARCAMAGE_LOG_REDACT=true
